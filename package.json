{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "material-ui": "^0.20.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.5",
    "react-test-renderer": "^16.8.2",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/assets/**",
      "!**/deploy/**",
      "!**/rules/**",
      "!**/package/**",
      "!**/mockdata/**",
      "!**/tools/**",
      "!**/prototypes/**",
      "!**/build/**",
      "!**/stories/**",
      "!**/.storybook/**",
      "src/**/*.js"
    ],
    "coverageReporters": [
      "text",
      "json",
      "json-summary",
      "lcov"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true,
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "redux-mock-store": "^1.5.3"
  }
}
